	<queryString>
		<![CDATA[SELECT
     exam.`ExamID` AS exam_ExamID,
     exam.`Grade` AS exam_Grade,
     exam.`ExamName` AS exam_ExamName,
     exam.`Subject` AS exam_Subject,
     face.`Index_No` AS face_Index_No,
