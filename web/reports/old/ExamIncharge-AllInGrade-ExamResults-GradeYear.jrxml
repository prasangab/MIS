	<queryString>
		<![CDATA[SELECT
     class_info.`Stu_ID` AS class_info_Stu_ID,
     class_info.`grade` AS class_info_grade,
     student.`FulName` AS student_FulName,
     face.`ExamID` AS face_ExamID,
     face.`Index_No` AS face_Index_No,
     face.`result` AS face_result,
     face.`chance` AS face_chance,
     face.`Year` AS face_Year,
     class_info.`class` AS class_info_class
FROM
     `student` student INNER JOIN `class_info` class_info ON student.`Stu_ID` = class_info.`Stu_ID`
     INNER JOIN `face` face ON student.`Stu_ID` = face.`Stu_ID`

where
class_info.`grade` =$P{Grade} and
face.`Year` = $P{Year}
