	<queryString>
		<![CDATA[SELECT
     involvestaff.`StaffID` AS involvestaff_StaffID,
     involvestaff.`ServiseID` AS involvestaff_ServiseID,
     involvestaff.`DateofAssigned` AS involvestaff_DateofAssigned,
     involvestaff.`Marks` AS involvestaff_Marks,
     involvestaff.`assigned` AS involvestaff_assigned,
     service.`Service_Name` AS service_Service_Name,
     staff.`FullName` AS staff_FullName,
     service.`Description` AS service_Description
FROM
     `staff` staff INNER JOIN `involvestaff` involvestaff ON staff.`Staff_ID` = involvestaff.`StaffID`
     INNER JOIN `service` service ON involvestaff.`ServiseID` = service.`ServiceID`
where
 involvestaff.`StaffID`=$P{staffID}]]>
